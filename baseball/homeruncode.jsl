Graph Builder(	Size( 534, 460 ),	Show Control Panel( 0 ),	Variables( X( :plate_x ), Y( :plate_z ) ),	Elements( Heatmap( X, Y, Legend( 9 ) ) ),	SendToReport(		Dispatch(			{},			"plate_x",			ScaleBox,			{Min( -4.7 ), Max( 4.7 ), Inc( 0.2 ), Minor Ticks( 1 )}		),		Dispatch(			{},			"plate_z",			ScaleBox,			{Min( 0 ), Max( 6 ), Inc( 0.2 ), Minor Ticks( 1 )}		),		Dispatch(			{},			"400",			ScaleBox,			{Legend Model(				9,				Properties(					0,					{gradient( {Color Theme( "White to Blue" ), Width( 12 )} )},					Item ID( "Count", 1 )				)			)}		),		Dispatch(			{},			"graph title",			TextEditBox,			{Set Text( "Location of Homerun Pitches" )}		)	));