Graph Builder(	Size( 569, 500 ),	Variables( X( :plate_x ), Y( :plate_z ) ),	Elements( Heatmap( X, Y, Legend( 5 ) ) ),	SendToReport(		Dispatch(			{},			"plate_x",			ScaleBox,			{Min( -4.7 ), Max( 4.7 ), Inc( 0.5 ), Minor Ticks( 1 )}		),		Dispatch(			{},			"plate_z",			ScaleBox,			{Min( 0 ), Max( 6 ), Inc( 0.5 ), Minor Ticks( 1 )}		),		Dispatch(			{},			"400",			ScaleBox,			{Legend Model(				5,				Properties(					0,					{gradient( {Color Theme( "White to Blue" )} )},					Item ID( "Count", 1 )				)			)}		),		Dispatch(			{},			"graph title",			TextEditBox,			{Set Text( "Pitch Location when the count is 0-2" )}		),		Dispatch(			{},			"X title",			TextEditBox,			{Set Text( "Horizontal Position from the Catcher's Perspective" )}		),		Dispatch(			{},			"Y title",			TextEditBox,			{Set Text( "Vertical Position from the Catcher's Perspective" )}		),		Dispatch(			{},			"Graph Builder",			FrameBox,			{Add Graphics Script(				4,				Description( "Script" ),				Fill Color( RGB Color( .7, .7, .7 ) );				Transparency( 10 );				Rect( -1, 1.25, 1, 3.5, /*fill:*/ 0 );			)}		)	));