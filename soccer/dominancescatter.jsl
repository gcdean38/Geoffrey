Graph Builder(	Variables( X( :appearances ), Y( :titles ), Color( :gender ) ),	Elements( Points( X, Y, Legend( 5 ), Jitter( "Random Uniform" ) ) ),	SendToReport(		Dispatch(			{},			"400",			ScaleBox,			{Legend Model(				5,				Properties( 1, {Line Color( 46 )}, Item ID( "Women", 1 ) )			)}		),		Dispatch(			{},			"graph title",			TextEditBox,			{Set Text( "Number of International Titles vs Appearances" )}		)	));